<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">

<module>

	<generate-with class='com.google.gwt.gwtai.applet.generator.AppletProxyGenerator'>
		<when-type-assignable class='com.google.gwt.gwtai.applet.client.Applet' />
	</generate-with>
	
	<define-linker name="jarlinker" class="com.google.gwt.gwtai.applet.linker.JarLinker" />

</module>
